-- CREAR TABLA
   CREATE TABLE USUARIOS
      (
        DNIUS         CHAR(9)  NOT NULL
       ,NOMBREUS      CHAR(20) NOT NULL
       ,APELLIDOSUS   CHAR(20) NOT NULL
       ,CLAVEUS       CHAR(8)  NOT NULL
       ,CARGOUS       CHAR(20) NOT NULL

       ,PRIMARY KEY (DNIUS)
      )

      IN P390DB.ESTUDITS;

   CREATE UNIQUE INDEX USUARIOS_IPK ON USUARIOS
      (
        DNIUS
      )
        CLUSTER CLOSE NO
        USING STOGROUP P390SG
        BUFFERPOOL BP1;
-- ELIMINAR TABLA
-- DROP TABLE P390.USUARIOS;
