-- *** TABLA DE PERFILES
-- CREAR TABLA
   CREATE TABLE PERFILES
      (
        TRANPERF         CHAR(5)  NOT NULL
       ,IDPERF           CHAR(3)  NOT NULL
       ,NOMBREPERF       CHAR(20) NOT NULL
       ,CONSULPERF       CHAR(2)  NOT NULL
       ,BORRARPERF       CHAR(2)  NOT NULL
       ,INGRESPERF       CHAR(2)  NOT NULL
       ,MODIFIPERF       CHAR(2)  NOT NULL

        ,PRIMARY KEY (TRANPERF,IDPERF)
       )

       IN P390DB.ESTUDITS;

    CREATE UNIQUE INDEX PERFILES_PK ON PERFILES
       (
         TRANPERF,IDPERF
        )
         CLUSTER CLOSE NO
         USING STOGROUP P390SG
         BUFFERPOOL BP1;
 -- ELIMINAR TABLA
 -- DROP TABLE P390.PERFILES;
