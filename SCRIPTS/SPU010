   CREATE TABLE PERMISOS
      (
        TRANPERM          CHAR(5)  NOT NULL
       ,IDPERM            CHAR(9)  NOT NULL
       ,IDPERFILPERM      CHAR(3)  NOT NULL
       ,NROACCESOSPERM    INTEGER
       ,FECHAULTPERM      CHAR(10)
       ,HORAULTPERM       CHAR(8)

       ,PRIMARY KEY (TRANPERM,IDPERM)
      )

      IN P390DB.ESTUDITS;

   CREATE UNIQUE INDEX PERMISOS_IPK ON PERMISOS
      (
         TRANPERM,IDPERM
       )
         CLUSTER CLOSE NO
         USING STOGROUP P390SG
         BUFFERPOOL BP1;
