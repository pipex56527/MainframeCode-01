-- CREAR TABLE SPACE
   CREATE TABLESPACE PERMISOS
      IN P390DB
     USING STOGROUP P390SG
     PRIQTY 250
     SECQTY 100
     ERASE NO
     FREEPAGE 10
     PCTFREE 10
     BUFFERPOOL BP1
     COMPRESS NO
     LOCKSIZE ANY
     LOCKMAX SYSTEM
     CLOSE YES;
-- BORRAR TABLE SPACE
-- DROP TABLESPACE P390DB.PERMISOS;
