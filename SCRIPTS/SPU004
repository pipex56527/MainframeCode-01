-- CREAR TABLA
   CREATE TABLE ESTUD
      (
        DNI           CHAR(9)  NOT NULL
       ,NOMBRE        CHAR(20) NOT NULL
       ,APELLIDOS     CHAR(20) NOT NULL
       ,DIRECCION     CHAR(35) NOT NULL

       ,PRIMARY KEY (DNI)
      )

      IN P390DB.ESTUDITS;

   CREATE UNIQUE INDEX ESTUD_IPK ON ESTUD
      (
        DNI
      )
        CLUSTER CLOSE NO
        USING STOGROUP P390SG
        BUFFERPOOL BP1;
-- ELIMINAR TABLA
-- DROP TABLE P390.ESTUD;
